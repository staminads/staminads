JWT_SECRET=your-secret-key-change-in-production
JWT_EXPIRES_IN=7d
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=changeme
PORT=3000

# Demo fixtures generation (optional)
DEMO_SECRET=your-demo-secret-change-in-production

# ClickHouse
CLICKHOUSE_HOST=http://localhost:8123
CLICKHOUSE_SYSTEM_DATABASE=staminads_system
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=

# Custom Dimensions Cache (optional)
CUSTOM_DIMENSIONS_CACHE_TTL_MS=15000

# Geo Location (optional)
# Path to MaxMind GeoLite2-City.mmdb database
# Download from: https://dev.maxmind.com/geoip/geolite2-free-geolocation-data
GEOIP_DB_PATH=./data/GeoLite2-City.mmdb
